grammar Basic

  rule optional_whitespace
    [\s]*
  end

  rule whitespace
    [\s]+
  end

  rule unquoted_string
    [a-zA-Z ]+
  end

  rule character
    .
  end

  rule integer
    zero / ( '-'? positive_number )
  end

  rule counting_number
    zero / positive_number
  end

  rule zero
    '0'
  end

  rule comma
    ','
  end

  rule positive_number
    [1-9] [0-9]*
  end

end
